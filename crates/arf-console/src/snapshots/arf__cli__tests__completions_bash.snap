    esac
}

if [[ "${BASH_VERSINFO[0]}" -eq 4 && "${BASH_VERSINFO[1]}" -ge 4 || "${BASH_VERSINFO[0]}" -gt 4 ]]; then
    complete -F _arf -o nosort -o bashdefault -o default arf
else
    complete -F _arf -o bashdefault -o default arf
fi
