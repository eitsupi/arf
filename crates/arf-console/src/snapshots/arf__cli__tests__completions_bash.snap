---
source: crates/arf-console/src/cli.rs
expression: completions
---
_arf() {
    local i cur prev opts cmd
    COMPREPLY=()
    if [[ "${BASH_VERSINFO[0]}" -ge 4 ]]; then
        cur="$2"
    else
        cur="${COMP_WORDS[COMP_CWORD]}"
    fi
    prev="$3"
    cmd=""
    opts=""

    for i in "${COMP_WORDS[@]:0:COMP_CWORD}"
    do
        case "${cmd},${i}" in
            ",$1")
                cmd="arf"
                ;;
            arf,completions)
                cmd="arf__completions"
                ;;
            arf,config)
                cmd="arf__config"
                ;;
            arf,help)
                cmd="arf__help"
                ;;
            arf,history)
                cmd="arf__history"
                ;;
            arf__config,help)
                cmd="arf__config__help"
                ;;
            arf__config,init)
                cmd="arf__config__init"
                ;;
            arf__config__help,help)
                cmd="arf__config__help__help"
                ;;
            arf__config__help,init)
                cmd="arf__config__help__init"
                ;;
            arf__help,completions)
                cmd="arf__help__completions"
                ;;
            arf__help,config)
                cmd="arf__help__config"
                ;;
            arf__help,help)
                cmd="arf__help__help"
                ;;
            arf__help,history)
                cmd="arf__help__history"
                ;;
            arf__help__config,init)
                cmd="arf__help__config__init"
                ;;
            arf__help__history,import)
                cmd="arf__help__history__import"
                ;;
            arf__help__history,schema)
                cmd="arf__help__history__schema"
                ;;
            arf__history,help)
                cmd="arf__history__help"
                ;;
            arf__history,import)
                cmd="arf__history__import"
                ;;
            arf__history,schema)
                cmd="arf__history__schema"
                ;;
            arf__history__help,help)
                cmd="arf__history__help__help"
                ;;
            arf__history__help,import)
                cmd="arf__history__help__import"
                ;;
            arf__history__help,schema)
                cmd="arf__history__help__schema"
                ;;
            *)
                ;;
        esac
    done

    case "${cmd}" in
        arf)
            opts="-e -c -q -h -V --eval --reprex --auto-format --config --no-banner --with-r-version --r-home --vanilla --quiet --no-save --save --no-restore --no-restore-data --restore-data --no-environ --no-site-file --no-init-file --interactive --no-readline --no-restore-history --no-auto-match --no-completion --history-dir --no-history --help --version [SCRIPT] completions config history help"
            if [[ ${cur} == -* || ${COMP_CWORD} -eq 1 ]] ; then
                COMPREPLY=( $(compgen -W "${opts}" -- "${cur}") )
                return 0
            fi
            case "${prev}" in
                --eval)
                    COMPREPLY=($(compgen -f "${cur}"))
                    return 0
                    ;;
                -e)
                    COMPREPLY=($(compgen -f "${cur}"))
                    return 0
                    ;;
                --config)
                    local oldifs
                    if [ -n "${IFS+x}" ]; then
                        oldifs="$IFS"
                    fi
                    IFS=$'\n'
                    COMPREPLY=($(compgen -f "${cur}"))
                    if [ -n "${oldifs+x}" ]; then
                        IFS="$oldifs"
                    fi
                    if [[ "${BASH_VERSINFO[0]}" -ge 4 ]]; then
                        compopt -o filenames
                    fi
                    return 0
                    ;;
                -c)
                    local oldifs
                    if [ -n "${IFS+x}" ]; then
                        oldifs="$IFS"
                    fi
                    IFS=$'\n'
                    COMPREPLY=($(compgen -f "${cur}"))
                    if [ -n "${oldifs+x}" ]; then
